.container {
  background-color: #F8F8F8;
}
.swiper {
  width: 100%;
}
.swiper-img {
  width: 100%;
  height: 300rpx;
}

.ts-box {
  width: 702rpx;
  height: 60rpx;
  margin-left: 24rpx;
  background-color: #FEF8ED;
  margin-top: 24rpx;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
}
.img-alert {
  width: 50rpx;
  height: 50rpx;
  margin-left: 5rpx;
  margin-top: 5rpx;
}
.ts-text {
  white-space: nowrap;
  text-indent: 12rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  width: 600rpx;
  font-size: 28rpx;
  color: #333333;
}
.img-right {
  width: 28rpx;
  height: 28rpx;
}

.row-card {
  widows: 702rpx;
  margin-top: 36rpx;
  display: flex;
  justify-content: space-around;
}
.row-card image {
  width: 336rpx;
  height: 180rpx;
}

.title {
  margin-top: 48rpx;
  margin-left: 24rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #333333;
}

.item-box {
  margin-left: 24rpx;
  margin-top: 20rpx;
}
.item-left-box {
  width: 339rpx;
  height: 509rpx;
  border-radius: 20rpx;
  background-color: #fff;
}
.item-img {
  width: 339rpx;
  height: 339rpx;
